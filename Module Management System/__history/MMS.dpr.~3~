program MMS;

uses
  Vcl.Forms,
  FormMain in 'FormMain.pas' {MainForm},
  FormAddModule in 'FormAddModule.pas' {addModulefrm};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TMainForm, MainForm);
  Application.CreateForm(TaddModulefrm, addModulefrm);
  Application.Run;
end.
